package tutorial;

public class Calculadora {

	public static void main(String[] args) {
		
		Calculadora calculadora = new Calculadora();
		
		int soma = 2 + 2;
		System.out.println(soma);
		
		soma = 4 + 3;
		System.out.println(soma);
		
	}
	public void somar(int n1, int n2)
	{
		int valor = n1 + n2;
		System.out.println(valor);
	}
	
	

}
