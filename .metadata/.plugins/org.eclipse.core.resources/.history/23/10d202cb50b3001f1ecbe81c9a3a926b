package Zoo;

public class App {

	public static void main(String[] args) {
		Gato gato = new Gato();
		Cachorro cao = new Cachorro ();
		Jacare jaca = new Jacare ();		
		
	    jaca.setRaca("Papo Amarelo");
	    jaca.setPeso(50);
		jaca.brincar();

		
		cao.emitirSom();
		cao.brincar();
		cao.setPeso(10);
		
		
		gato.setCor("Branco");
	    gato.setRaca("Vira Lata");
	    gato.setPeso(3);
		gato.saltar();
		gato.emitirSom();
		gato.brincar();
	}

}
