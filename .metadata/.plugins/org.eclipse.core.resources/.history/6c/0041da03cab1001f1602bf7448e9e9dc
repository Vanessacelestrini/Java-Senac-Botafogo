package Zoo;

public class App {

	public static void main(String[] args) {
		Gato gato = new Gato();
		
		
		gato.setCor("Branco");
	    gato.setRaca("Vira Lata");
	    gato.setPeso(-4);
		gato.saltar();
		gato.emitirSom();
		gato.brincar();
		gato.info();
		gato.classificarPeso();

	}

}
